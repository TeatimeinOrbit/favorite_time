<% if users.exists? %>
  <% users.each do |user| %>
    <table>
      <thead>
        <tr>
          <th>Name</th>
        </tr>
      </thead>
      <tboby>
        <tr>
          <td><%= image_tag user.get_profile_image(100, 100) %></td>
          <td><%= user.name %></td>
        </tr>
      </tboby>
    </table>
  <% end %>
<% else %>
  <p>ユーザーはまだいません</p>
<% end %>